  "insertSim": [{
      "root": "Insert an Activated SIM",
      "top_description": "If setting up cellular connectivity, do the following:",
      "bottom_description": "NOTE: The modem door cover must be on the device or the embedded modem will not power on.",
      "children": [{
          "type": "string",
          "value": "Remove the modem door covering the SIM slots.",
          "children": null
        },
        {
          "type": "string",
          "value": "Insert an activated 2FF-sized SIM card with metal contacts face up and notch in, into the SIM 1 slot. It will click into place. Depress again to remove, if necessary.",
          "children": [{
            "type": "string",
            "value": "Insert a secondary/backup, activated SIM card   into the SIM 2 slot, if desired.",
            "children": null
          }]
        },
        {
          "type": "string",
          "value": "Replace the modem door and secure with the modem door screw.",
          "children": null
        },
        {
          "type": "image",
          "value": "images/routers/aer2200_sims.jpg",
          "children": null
        }

      ]
    },
    {
      "root": "Insert Optional External MC400 Modem",
      "top_description": "The AER2200 has a slot for second, removable modem (MC400). Before the AER2200 can be set up, the additional modem must be installed. To install this modem, do the following:",
      "bottom_description": null,
      "children": [{
          "type": "string",
          "value": "Insert an activated 2FF-sized SIM card into the MC400 modem, metal contacts down, notch end first.",
          "children": null
        },
        {
          "type": "image",
          "value": "images/routers/aer2200_mc400_sim_slots.jpg",
          "children": null
        },
        {
          "type": "string",
          "value": "Remove the modem door on the AER2200 device.",
          "children": null
        },
        {
          "type": "image",
          "value": "images/routers/aer2200_mc400_slot.jpg",
          "children": null
        },
        {
          "type": "string",
          "value": "Slide the MC400 modem into the available modem slot, USB plug first, with the Cradlepoint logo facing up.",
          "children": null
        },
        {
          "type": "string",
          "value": "Replace the MC400 modem door cover. NOTE:&nbsp;Remove the plastic SMA covers from the modem door cover for the additional antenna ports on the modem.",
          "children": null
        },
        {
          "type": "image",
          "value": "images/routers/aer2200_mc400_slot_alt.jpg",
          "children": null
        },
        {
          "type": "string",
          "value": "Secure with the door cover screw (beneath the device).",
          "children": null
        }

      ]
    }
  ],
  "attachAnttena": [{
    "root": "Attach the Antennas",
    "top_description": "Attach the four WiFi antennas (included) and two or four cellular modem antennas (included) to the connectors.",
    "bottom_description": "NOTE: Cellular antennas include RP-SMA connectors (pin in the center on the antenna) while WiFi antennas include SMA connectors (pin in the center on the device connection point). Ensure you connect the correct antennas to their applicable connection locations.",
    "children": [{
        "type": "string",
        "value": "Hold the antenna straight and twist the base of the antenna to connect to the device.",
        "children": [{
          "type": "string",
          "value": "Do not over-tighten. The maximum recommended torque is 4 in-lbs.",
          "children": null
        }]
      },
      {
        "type": "image",
        "value": "images/routers/aer2200_antennas.jpg",
        "children": null
      },
      {
        "type": "string",
        "value": "Angle the antennas for optimal signal reception.",
        "children": null
      },
      {
        "type": "image",
        "value": "images/routers/aer2200_wifi_antennas.jpg",
        "children": null
      }

    ]
  }],
