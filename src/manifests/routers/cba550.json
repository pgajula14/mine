  "insertSim": [
    {
      "root": "Insert an Activated SIM",
      "top_description": null,
      "bottom_description": null,
      "children": [
        {
          "type": "string",
          "value": "Slide the SIM door cover to the left to reveal the SIM slots.",
          "children": null
        },
        {
          "type": "string",
          "value": "Insert the SIM into the slot marked SIM-1 with the metal contacts up and notch in. It will click into place. Depress the SIM card to remove it.",
          "children": null
        },
        {
          "type": "image",
          "value": "images/routers/cba550_sims.jpg",
          "children": null
        },
        {
          "type": "string",
          "value": "Use SIM-2 for secondary/backup SIM.",
          "children": null
        },
        {
          "type": "image",
          "value": "images/routers/cba550_sim_door.jpg",
          "children": null
        },
        {
          "type": "string",
          "value": "Slide the SIM door cover to the right to cover the SIM slots.",
          "children": null
        }
      ]
    },
    {
      "root": "Insert External MC400 Modem (optional)",
      "top_description": null,
      "bottom_description": null,
      "children": [
        {
          "type": "string",
          "value": "The IBR1700 has a slot for second, removable modem (MC400).",
          "children": null
        },
        {
          "type": "string",
          "value": "Insert an activated 2FF-sized SIM card into the MC400 modem, metal contacts down, notch end first.",
          "children": null
        },
        {
          "type": "image",
          "value": "images/routers/ibr1700_mc400_slot.jpg",
          "children": null
        },
        {
          "type": "string",
          "value": "Remove the modem door on the IBR1700 device.",
          "children": null
        },
        {
          "type": "string",
          "value": "Slide the MC400 modem into the available modem slot, USB plug first, with the Cradlepoint logo facing up.",
          "children": null
        },
        {
          "type": "string",
          "value": "Replace the MC400 modem door cover.",
          "children": [
            {
              "type": "string",
              "value": "NOTE: Remove the plastic SMA covers from the modem door cover for the additional antenna ports on the modem.",
              "children": null
            }
          ]
        },
        {
          "type": "string",
          "value": "Secure with the door cover screw (beneath the device).",
          "children": null
        }
      ]
    }
  ],
